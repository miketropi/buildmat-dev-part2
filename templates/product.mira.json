        "164724157579bc979a-1": {
          "type": "liquid",
          "disabled": true,
          "settings": {
            "code": "{% if product.metafields.my_fields.product_specific_description != blank %} \n<p>\n{{ product.metafields.my_fields.product_specific_description }}<\/p>\n{% else %}\n{{ \" \"}}\n{% endif %}",
